public class SynceContactsFuture {
    @future
    public static void setContactsSynce () {
        List<Account> accounts = [SELECT Id, BillingAddress,
                                 (SELECT Id, Is_Synced__c, Processed_By_Future__c
                                  FROM Contacts)
                                  FROM Account LIMIT 150];
        List<Contact> contacts = new List<Contact>();
        for (Account account: accounts) {
            if (account.BillingAddress!=null) {
                for (Contact contact: account.contacts) {
                    contact.Is_Synced__c = false;
                    contact.Processed_By_Future__c = true;
                    contacts.add(contact);
                }
            }
        }
        update contacts;
    }

}
